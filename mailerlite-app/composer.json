{
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5",
        "pestphp/pest": "^2.33",
        "pestphp/pest-plugin-stressless": "^2.2",
        "pestphp/pest-plugin-faker": "^2.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "tightenco/collect": "^9.52",
        "predis/predis": "^2.2",
        "guzzlehttp/guzzle": "^7.8",
        "fakerphp/faker": "^1.23"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
